{
   "name": "io.krunker.desktop",
   "version": "1.8.7",
   "description": "Desktop Client For Krunker",
   "author": "Yendis Entertainment Pty Ltd",
   "main": "app/main.js",
   "scripts": {
      "start": "electron .",
      "pack": "electron-builder --dir",
      "dist": "electron-builder"
   },
   "build": {
      "productName": "Official Krunker.io Client",
      "appId": "io.krunker.desktop",
      "win": {
         "target": [
            "portable"
         ]
      },
      "mac": {
         "category": "public.app-category.games"
      },
      "portable": {
         "artifactName": "Official Krunker.io Client.exe"
      }
   },
   "devDependencies": {
      "electron": "^9.0.0",
      "electron-builder": "^22.6.1"
   },
   "dependencies": {
      "discord-rpc": "^3.1.0",
      "electron-localshortcut": "^3.2.1",
      "electron-log": "^4.1.0",
      "electron-store": "^5.1.1",
      "electron-updater": "^4.2.2",
      "rimraf": "^3.0.1",
      "v8-compile-cache": "^2.1.0"
   }
}
